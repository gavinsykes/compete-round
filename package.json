{
  "author": "Gavin Sykes <gavin@gavinsykes.uk> (https://gavinsykes.uk/)",
  "bugs": {
    "url": "https://github.com/gavinsykes/complete-round/issues"
  },
  "description": "A function to offer more rounding options for numbers in JavaScript and TypeScript",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.4",
    "@types/webpack-env": "^1.15.2",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^7.1.2",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "shx": "^0.3.2",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.7",
    "typedoc-webpack-plugin": "^1.1.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "homepage": "https://github.com/gavinsykes/complete-round#readme",
  "keywords": [
    "math",
    "mathematics",
    "maths",
    "number",
    "numbers",
    "round",
    "rounding"
  ],
  "license": "MIT",
  "main": "dist/main.js",
  "name": "complete-round",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinsykes/complete-round.git"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "shx rm -rf dist/ docs/",
    "cover": "istanbul cover _mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "docs": "typedoc",
    "clean-slate-protocol-sir": "shx rm -rf node_modules/ dist/ docs/",
    "postversion": "git push origin master && git push origin master --tags",
    "setup": "npm install",
    "test": "mocha -r ts-node/register dev/**/*.spec.ts",
    "test-travis": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec ./dev/test/*",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./dev/"
  },
  "types": "./dist/index.d.ts",
  "version": "1.1.6"
}
